{
  "openapi": "3.0.0",
  "info": {
    "title": "Table Tennis Player API Design",
    "version": "1.0.0",
    "description": "Design document for the Table Tennis Player API"
  },
  "paths": {
    "/api/players": {
      "get": {
        "summary": "Returns a list of players",
        "responses": {
          "200": {
            "description": "A list of players",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Player"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Player": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          }
          
        }
      }
    }
  },
  "tags": []
}
